<% @groups.each do |group| %>
<h1>GRUPO - <%= group.id %></h1>
<h2>Mestre - <%= group.master.nickname %> </h2>
  <% group.group_members.each do |member|%>
    <% if member.profile == group.master %>
      <h3><b> <%= member.profile.nickname %> </h3></b>
    <% else %>
      <h3> <%= member.profile.nickname %> </h3>
    <% end %>
    <p><%= member.profile.city %> </p>
  <% end %>
<% end %>

<%= link_to "Back to Profiles", profiles_path%>
